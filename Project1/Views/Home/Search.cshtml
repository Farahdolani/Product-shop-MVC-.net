@model List<Product>
<table class="table table-striped font-weight-bold">
    <thead>
    <th>ID</th>
    <th>Product Name</th>
    <th> Price</th>
    <th>Delete</th>
    <th>Edit</th>
    </thead>
    @foreach (var c in Model)
    {
        <tr class="m-3">
            <td>@c.ProductId</td>
            <td>@c.ProductName</td>
            <td>@c.ProducrPrise</td>
           

        </tr>
    }

</table>
<a asp-controller="Login" asp-action="BackToCategory" class="btn btn-primary"> Back To Category</a>